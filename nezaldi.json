{
  "debug": true,
  "dashboard": {
    "enable": true
  },
  "rules": [
    {
      "path": "^/a$",
      "resetPath": true,
      "target": "http://localhost:4000/index.html"
    },
    {
      "path": "^/redirect$",
      "redirect": "https://www.google.com"
    },
    {
      "path": "^/hello/?$",
      "resetPath": true,
      "accept": "text/html",
      "target": "http://localhost:4000/hello",
      "addHeaders": [
        {
          "name": "X-MyHeader",
          "value": "HelloWorld"
        }
      ]
    },
    {
      "path": "^/a/([^/]+)/h([^/]+)$",
      "target": "http://localhost:4000/z/$1$2"
    },
    {
      "path": "^/static/",
      "target": "./src/dashboard/build/"
    },
    {
      "path": "^/",
      "target": "http://localhost:8000/src/dashboard/web/",
      "removeHeaders": ["host"],
      "addHeaders": [
        {
          "name": "host",
          "value": "localhost:8000"
        }
      ]
    }
  ]
}